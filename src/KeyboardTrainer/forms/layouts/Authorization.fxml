<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="226.0" prefWidth="455.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="KeyboardTrainer.forms.controllers.login.AuthorizationController">
    <Label layoutX="108.0" layoutY="14.0" text="Авторизация">
        <font>
            <Font size="40.0"/>
        </font>
    </Label>
    <TextField fx:id="loginTextField" layoutX="133.0" layoutY="101.0" prefHeight="25.0" prefWidth="190.0"
               promptText="Логин"/>
    <PasswordField fx:id="passwordTextField" layoutX="133.0" layoutY="141.0" prefHeight="25.0" prefWidth="190.0"
                   promptText="Пароль"/>
    <Button fx:id="loginButton" layoutX="274.0" layoutY="176.0" mnemonicParsing="false" text="Войти"/>
    <Button fx:id="registerButton" layoutX="133.0" layoutY="176.0" mnemonicParsing="false" text="Регистрация"/>
</AnchorPane>
