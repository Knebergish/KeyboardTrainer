<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="258.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="KeyboardTrainer.forms.controllers.login.RegistrationController">
    <Label layoutX="110.0" layoutY="20.0" text="Регистрация">
        <font>
            <Font size="40.0"/>
        </font>
    </Label>
    <TextField fx:id="loginTextField" layoutX="125.0" layoutY="96.0" prefHeight="25.0" prefWidth="195.0"
               promptText="Логин"/>
    <PasswordField fx:id="primoPasswordField" layoutX="125.0" layoutY="133.0" prefWidth="195.0" promptText="Пароль"/>
    <PasswordField fx:id="secundoPasswordField" layoutX="125.0" layoutY="169.0" prefWidth="195.0"
                   promptText="Подтверждение"/>
    <Button fx:id="registerButton" layoutX="190.0" layoutY="207.0" mnemonicParsing="false" prefHeight="25.0"
            prefWidth="130.0" text="Зарегистрироваться"/>
    <Button fx:id="cancelButton" layoutX="125.0" layoutY="207.0" mnemonicParsing="false" text="Отмена"/>
</AnchorPane>
